@model IEnumerable<HoangHuyTuan_5951071116.Models.Employee>
@{
    ViewData["Title"] = "Home Page";
}
<table class="table">
    <thead>
        <tr>
            <th>Sr No.</th>
            <th>Employee Name</th>
            <th>City</th>
            <th>State</th>
            <th>Country</th>
            <th>Department</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Sr_no</td>
                <td>@item.Emp_name</td>
                <td>@item.City</td>
                <td>@item.State</td>
                <td>@item.Country</td>
                <td>@item.Department</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Sr_no">Edit</a>
                    <a asp-action="Delete" asp-route-id="@item.Sr_no">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>